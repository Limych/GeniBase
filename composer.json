{
	"name" : "limych/genibase",
	"description" : "GeniBase is web software you can use to create a genealogical databases.",
	"license" : "GPL-3.0",
	"type" : "project",
	"require" : {
		"php" : ">=5.5.9",
		"gedcomx/gedcomx-php" : "dev-3.1.x-fix",
		"silex/silex" : "~2.1",
		"symfony/monolog-bridge" : "~2.8|^3.0",
		"symfony/twig-bridge" : "~2.8|^3.0",
		"symfony/console" : "~2.8|^3.0",
		"nesbot/carbon" : "~1.22",
		"symfony/web-link" : "~3.3",
		"symfony/config" : "~2.8|^3.0",
		"symfony/http-foundation" : "~3.3",
		"doctrine/dbal" : "~2.5",
		"euskadi31/cors-service-provider" : "~1.0",
		"symfony/serializer" : "~2.1|~3.0",
		"symfony/asset" : "~3.3"
	},
	"require-dev" : {
		"phpunit/phpunit" : "*",
		"silex/web-profiler" : "~2.0",
		"symfony/debug" : "~2.8|^3.0",
		"phploc/phploc" : "~2.1 || ~4.0",
		"cyberspectrum/pharpiler" : "~1.0",
		"satooshi/php-coveralls" : "~1.0",
		"php-console/php-console" : "~3.1",
		"symfony/browser-kit" : "~2.1|~3.0",
		"texthtml/whoops-silex" : "~1.0",
		"squizlabs/php_codesniffer" : "*",
		"phpmd/phpmd" : "*",
		"sebastian/phpcpd" : "*",
		"sebastian/phpdcd" : "*"
	},
	"authors" : [{
			"name" : "Andrey Khrolenok",
			"email" : "andrey@khrolenok.ru",
			"homepage" : "https://github.com/Limych/GeniBase"
		}
	],
	"minimum-stability" : "stable",
	"support" : {
		"issues" : "https://github.com/Limych/GeniBase/issues",
		"wiki" : "https://github.com/Limych/GeniBase/wiki"
	},
	"autoload" : {
		"psr-4" : {
			"" : "src",
			"GeniBase\\Tests\\" : "tests/phpunit/includes"
		}
	},
	"scripts" : {
		"run" : [
			"echo 'Started web server on http://localhost:8888'",
			"php -S localhost:8888 -t web"
		],
		"sca" : [
			"php vendor/bin/phpcs app src tests/phpunit/tests --standard=build/config/phpcs.xml --ignore=*.html.php,*.config.php,*.twig.php",
			"php vendor/bin/phpmd app text build/config/phpmd.xml",
			"php vendor/bin/phpmd src text build/config/phpmd.xml",
			"php vendor/bin/phpmd tests/phpunit/tests text build/config/phpmd.xml"
		]
	},
	"homepage" : "https://github.com/Limych/GeniBase",
	"repositories" : [{
			"type" : "git",
			"url" : "https://github.com/Limych/gedcomx-php",
			"name" : "gedcomx/gedcomx-php"
		}
	]
}